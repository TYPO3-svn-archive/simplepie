									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:simplepie/Resources/Private/Language/locallang_db.xml:options.scaleMode.disproportionally</numIndex>
										<numIndex index="1">DISPROPORTIONALLY</numIndex>
